	TYPE_MISMATCH, 
	OTHER_EXCEPTIONS,
	BODY_MISSING_IN_REQUEST,
	PATH_VARIABLE_MISSING_IN_REQUEST,
	METHOD_DOES_NOT_EXISTS_WITH_BANK_DEMO_API,
	MEDIA_TYPE_NOT_SUPPORTED,
	MEDIA_TYPE_NOT_ACCEPTABLE,
	INTERNAL_SERVER_ERROR,
	MEDIA_CONVERSION_NOT_SUPPORTED,
	NO_CUSTOMERS_EXISTS_WITH_BANK,
	NO_CUSTOMER_EXISTS_WITH_PROVIDED_ID,
	NO_ACCOUNTS_EXISTS_WITH_BANK,
	NO_ACCOUNT_EXISTS_WITH_PROVIDED_ID
}








