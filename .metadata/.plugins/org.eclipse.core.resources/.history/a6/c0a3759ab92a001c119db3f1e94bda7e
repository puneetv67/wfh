package com.project.ipldashboard.ipldashboarddemo.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class Match {
	@Id
	private String id;
	private String team1;
	private String team2;
	private String playerOfTheMatch;
	private String tossWinner;
	private String tossDecision;
	private String result;
	private String resultMargin;

	public Match(){
		
	}
	
	public Match(String id, String team1, String team2, String playerOfTheMatch, String tossWinner, String tossDecision,
			String result, String resultMargin, String umpire1, String umpire2, String venue, String city,
			String date) {
		super();
		this.id = id;
		this.team1 = team1;
		this.team2 = team2;
		this.playerOfTheMatch = playerOfTheMatch;
		this.tossWinner = tossWinner;
		this.tossDecision = tossDecision;
		this.result = result;
		this.resultMargin = resultMargin;
		this.umpire1 = umpire1;
		this.umpire2 = umpire2;
		this.venue = venue;
		this.city = city;
		this.date = date;
	}


	public String getPlayerOfTheMatch() {
		return playerOfTheMatch;
	}

	public void setPlayerOfTheMatch(String playerOfTheMatch) {
		this.playerOfTheMatch = playerOfTheMatch;
	}

	public String getTossWinner() {
		return tossWinner;
	}

	public void setTossWinner(String tossWinner) {
		this.tossWinner = tossWinner;
	}

	public String getTossDecision() {
		return tossDecision;
	}

	public void setTossDecision(String tossDecision) {
		this.tossDecision = tossDecision;
	}

	public String getResultMargin() {
		return resultMargin;
	}

	public void setResultMargin(String resultMargin) {
		this.resultMargin = resultMargin;
	}

	private String umpire1;
	private String umpire2;
	private String venue;
	private String city;
	private String date;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getTeam1() {
		return team1;
	}

	public void setTeam1(String team1) {
		this.team1 = team1;
	}

	public String getTeam2() {
		return team2;
	}

	public void setTeam2(String team2) {
		this.team2 = team2;
	}

	public String getResult() {
		return result;
	}

	public void setResult(String result) {
		this.result = result;
	}

	public String getUmpire1() {
		return umpire1;
	}

	public void setUmpire1(String umpire1) {
		this.umpire1 = umpire1;
	}

	public String getUmpire2() {
		return umpire2;
	}

	public void setUmpire2(String umpire2) {
		this.umpire2 = umpire2;
	}

	public String getVenue() {
		return venue;
	}

	public void setVenue(String venue) {
		this.venue = venue;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public String getDate() {
		return date;
	}

	public void setDate(String date) {
		this.date = date;
	}

}
